{
	"name": "font-picker",
	"version": "2.0.3",
	"description": "Font selector component for Google Fonts",
	"keywords": [
		"fonts",
		"google fonts",
		"selector",
		"picker",
		"component",
		"preview",
		"download"
	],
	"main": "lib/font-picker.js",
	"author": "Samuel Meuli <dev@samuelmeuli.com>",
	"repository": "github:samuelmeuli/font-picker",
	"homepage": "https://samuelmeuli.github.io/font-picker",
	"license": "MIT",
	"scripts": {
		"start": "npm-run-all build --parallel start:*",
		"start:rollup": "rollup --config --watch",
		"start:serve": "serve demo --cache=0 --open",
		"build": "rollup --config",
		"lint": "eslint .",
		"precommit": "npm run lint",
		"prepush": "npm run lint"
	},
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-plugin-external-helpers": "^6.22.0",
		"babel-preset-env": "^1.6.1",
		"eslint": "^4.19.1",
		"eslint-config-airbnb-base": "^12.1.0",
		"eslint-plugin-import": "^2.9.0",
		"husky": "^0.14.3",
		"node-sass": "^4.8.3",
		"npm-run-all": "^4.1.2",
		"postcss": "^6.0.21",
		"rollup": "^0.57.1",
		"rollup-plugin-babel": "^3.0.3",
		"rollup-plugin-postcss": "^1.4.0",
		"serve": "^6.5.5"
	},
	"files": [
		"lib/"
	]
}
